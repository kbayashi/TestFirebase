<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="20dp"
    tools:context=".UserProfileSpinnerEditDialog">

    <Spinner
        android:id="@+id/activity_user_profile_spinner_edit_spinner"
        android:layout_width="271dp"
        android:layout_height="52dp"
        android:layout_marginBottom="32dp"
        android:spinnerMode="dialog"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/activity_user_profile_spinner_edit_title_textView"
        app:layout_constraintVertical_bias="0.398" />

    <Button
        android:id="@+id/activity_user_profile_spinner_edit_cancel_button"
        android:layout_width="143dp"
        android:layout_height="55dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="32dp"
        android:text="キャンセル"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/activity_user_profile_spinner_edit_save_button"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/activity_user_profile_spinner_edit_spinner" />

    <Button
        android:id="@+id/activity_user_profile_spinner_edit_save_button"
        android:layout_width="143dp"
        android:layout_height="55dp"
        android:layout_marginStart="105dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="32dp"
        android:text="保存"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/activity_user_profile_spinner_edit_cancel_button"
        app:layout_constraintTop_toBottomOf="@+id/activity_user_profile_spinner_edit_spinner" />

    <TextView
        android:id="@+id/activity_user_profile_spinner_edit_title_textView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>